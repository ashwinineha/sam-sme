{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sme-imp-df"
		},
		"ccv1_media_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ccv1_media'"
		},
		"d1_sftp_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'd1_sftp'"
		},
		"ccv1_media_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "100.101.1.4"
		},
		"ccv1_media_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "migrate"
		},
		"d1_sftp_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "157.133.126.10"
		},
		"d1_sftp_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "transfer_es"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ccv1_media_password": {
						"value": "[parameters('ccv1_media_password')]"
					},
					"d1_sftp_password": {
						"value": "[parameters('d1_sftp_password')]"
					},
					"ccv1_media_properties_typeProperties_host": {
						"value": "[parameters('ccv1_media_properties_typeProperties_host')]"
					},
					"ccv1_media_properties_typeProperties_userName": {
						"value": "[parameters('ccv1_media_properties_typeProperties_userName')]"
					},
					"d1_sftp_properties_typeProperties_host": {
						"value": "[parameters('d1_sftp_properties_typeProperties_host')]"
					},
					"d1_sftp_properties_typeProperties_userName": {
						"value": "[parameters('d1_sftp_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}